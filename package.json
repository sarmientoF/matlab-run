{
  "name": "matlab-code-run",
  "displayName": "Matlab Code Run",
  "description": "Enables code execution in VS Code via integrated terminal",
  "version": "1.0.1",
  "repository": {
    "url": "https://github.com/sarmientoF/matlab-run",
    "type": "GitHub"
  },
  "publisher": "sarmientoF",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:matlab"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.runMatlab",
        "title": "Run Matlab File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
